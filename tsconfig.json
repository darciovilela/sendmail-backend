    "baseUrl": "./src",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "rootDir": "./src",
    "outDir": "./build",
  },
  "include": [
      "src/**/*"
  ]
}
